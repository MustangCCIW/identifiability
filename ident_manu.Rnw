data(sens_ests_cat_all)

tmp <- reshape2::melt(sens_ests_cat_all, id.var = c('Parameter', 'error', 'Category'))

lims <- with(tmp, quantile(error, c(0.05, 0.95))) %>% 
  as.numeric

stts_lev <- rev(c('NH4', 'Chla_mg_tot', 'O2', 'irradiance', 'NO3', 'OM1', 'OM2', 'PO4'))
stts_lab <- rev(c('Ammonium', 'Chlorophyll', 'Dissolved Oxygen', 'Irradiance', 'Nitrate', 'OM1', 'OM2', 'Phosphate'))

toplo <- filter(tmp, error < lims[2] & error > lims[1]) %>% 
  mutate(
    L1 = factor(L1, levels = stts_lev, labels = stts_lab)
  ) %>% 
  rename(Sensitivity = error)

toplo1 <- filter(toplo, !Category %in% c('Phytoplankton', 'Zooplankton')) %>% 
  mutate(Parameter = as.character(par_txt(Parameter, frm = 'exp')))
toplo2 <- filter(toplo, Category %in% c('Phytoplankton')) %>% 
  mutate(
    grp = gsub('^.*_([1-6])$', 'p\\1', Parameter), 
