    ), 
  Parameter = as.character(Parameter)
  ) %>% 
  group_by(Category) %>% 
  mutate(
    tops = ifelse(error == max(error), Parameter, NA)
  )
  
ylab <- expression(paste(O[2], ' (mmol ', m^-3, ')'))
