  geom_tile() + 
  pbase +
  scale_x_discrete(expand = c(0, 0), 
    breaks = toplo2$Parameter, 
    labels = parse(text = toplo2$Parameter)
    ) + 
  scale_y_discrete(expand = c(0, 0)) +
  scale_fill_distiller(palette = "Spectral", limits = lims) + 
  facet_grid(~grp, scales = 'free') + 
  ggtitle('Phytoplankton')
p3 <- ggplot(toplo3, aes(x = Parameter, y = L1, fill = Sensitivity)) + 
  geom_tile() + 
  pbase +
  scale_x_discrete(expand = c(0, 0), 
